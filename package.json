  "jest": {
    "moduleNameMapper": {
      "^@config/(.*)$": "<rootDir>/modules/config/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1",
      "^@user/(.*)$": "<rootDir>/modules/user/$1",
      "^@media/(.*)$": "<rootDir>/modules/media/$1",
      "^@file/(.*)$": "<rootDir>/modules/file/$1",
      "^@xml-data-bridge/(.*)$": "<rootDir>/modules/xml-data-bridge/$1",
      "^@explorer/(.*)$": "<rootDir>/modules/explorer/$1",
      "^@auth/(.*)$": "<rootDir>/modules/auth/$1",
      "^@captcha/(.*)$": "<rootDir>/modules/captcha/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "coveragePathIgnorePatterns": [
      ".mock.ts",
      ".e2e-spec.ts",
      ".module.ts",
      ".entity.ts",
      ".subscriber.ts",
      "main.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/"
    ]
  }

  "scripts": {
    "build": "rimraf dist && tsc -p ./src/tsconfig.lib.json",
    "create:tar": "node ./create-pack-list.js",
    "publish": "npm publish ./dist/package.tgz",
    "format": "prettier --write \"src/**/*.ts\"",
    "init": "npm run init:config && npm run init:web-app",
    "init:config": "nest start init-config --debug",
    "init:web-app": "nest start init-web-app",
    "build:web-app": "nest build web-app",
    "lint": "eslint \"{src,app,modules,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,app,modules,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config web/test/jest-e2e.json"
  },

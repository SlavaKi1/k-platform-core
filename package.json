{
  "name": "k-platform-core",
  "version": "0.0.1",
  "description": "",
  "author": "Alexander Kiriliuk",
  "license": "Apache-2.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build:config": "nest start config",
    "start:composer": "nest start composer",
    "start:composer:dev": "nest start composer --watch",
    "start:composer:debug": "nest start composer --debug --watch",
    "start:composer:prod": "node dist/apps/composer/src/main",
    "start:auth": "nest start auth",
    "start:auth:dev": "nest start auth --watch",
    "start:auth:debug": "nest start auth --debug --watch",
    "start:auth:prod": "node dist/apps/auth/src/main",
    "start:captcha": "nest start captcha",
    "start:captcha:dev": "nest start captcha --watch",
    "start:captcha:debug": "nest start captcha --debug --watch",
    "start:captcha:prod": "node dist/apps/captcha/src/main",
    "start:user": "nest start user",
    "start:user:dev": "nest start user --watch",
    "start:user:debug": "nest start user --debug --watch",
    "start:user:prod": "node dist/apps/user/src/main",
    "start:media": "nest start media",
    "start:media:dev": "nest start media --watch",
    "start:media:debug": "nest start media --debug --watch",
    "start:media:prod": "node dist/apps/media/src/main",
    "start:files": "nest start files",
    "start:files:dev": "nest start files --watch",
    "start:files:debug": "nest start files --debug --watch",
    "start:files:prod": "node dist/apps/files/src/main",
    "start:explorer": "nest start explorer",
    "start:explorer:dev": "nest start explorer --watch",
    "start:explorer:debug": "nest start explorer --debug --watch",
    "start:explorer:prod": "node dist/apps/explorer/src/main",
    "start:db-script-evaluator": "nest start db-script-evaluator",
    "start:db-script-evaluator:dev": "nest start db-script-evaluator --watch",
    "start:db-script-evaluator:debug": "nest start db-script-evaluator --debug --watch",
    "start:db-script-evaluator:prod": "node dist/apps/db-script-evaluator/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/auth/test/jest-e2e.json"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "9.0.5",
    "@nestjs/common": "9.3.10",
    "@nestjs/core": "9.3.10",
    "@nestjs/jwt": "10.0.2",
    "@nestjs/microservices": "9.3.10",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.3.10",
    "bcrypt": "5.1.0",
    "canvas": "2.11.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.0.3",
    "helmet": "6.0.1",
    "imagemin": "7.0.1",
    "imagemin-pngquant": "8.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "ioredis": "5.3.1",
    "jwt-simple": "0.5.6",
    "multer": "1.4.5-lts.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.10.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "sharp": "0.32.0",
    "typeorm": "0.3.12",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.10",
    "@nestjs/typeorm": "9.0.1",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.3.1",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleNameMapper": {
      "^@config/(.*)$": "<rootDir>/apps/config/$1",
      "^@shared/(.*)$": "<rootDir>/apps/shared/$1",
      "^@user/(.*)$": "<rootDir>/apps/user/$1",
      "^@media/(.*)$": "<rootDir>/apps/media/$1",
      "^@files/(.*)$": "<rootDir>/apps/files/$1",
      "^@db-script-evaluator/(.*)$": "<rootDir>/apps/db-script-evaluator/$1",
      "^@explorer/(.*)$": "<rootDir>/apps/explorer/$1",
      "^@composer/(.*)$": "<rootDir>/apps/composer/$1",
      "^@auth/(.*)$": "<rootDir>/apps/auth/$1",
      "^@captcha/(.*)$": "<rootDir>/apps/captcha/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
